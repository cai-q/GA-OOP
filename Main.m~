GA = Strategy.GA;
LBGA = Strategy.LBGA;
ALBGA = Strategy.ALBGA;

           
            
table = array2table(res, 'VariableNames', {...
    'Generation', 'GA-BestFitness', 'GA-AverageFitness'});
writetable(table, 'result.txt', 'Delimiter', '\t', 'WriteRowNames', true);

figure;
plot(res(:, 1), res(:, 2), res(:, 1), res(:, 3));
title('GA (Genetic Algorithm)');
xlabel('Generation');
ylabel('Fitness');
legend('best', 'average');
saveas(gcf, 'GA.png');